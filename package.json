{
    "name": "flask-react-boilerplate",
    "version": "0.1.0",
    "author": "Yale DH Lab",
    "license": "MIT",
    "description": "Minimal boilerplate for a Flask + React application",
    "main": "server/server.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/duhaime/react-boilerplate.git"
    },
    "scripts": {
        "start": "webpack-dev-server --port 7081 --history-api-fallback",
        "dev": "webpack-dev-server --port 7081 --history-api-fallback",
        "build": "webpack --config webpack.config.js",
        "compress": "webpack --config webpack.config.js",
        "production": "npm run build && npm run serve",
        "serve": "FLASK_APP=server/app.py flask run --port=7082",
        "lint-js": "eslint -c './src/config/.eslintrc.js' src/",
        "lint-py": "pylint server/app.py",
        "lint": "npm run lint-js && npm run lint-py",
        "jest": "jest",
        "test": "npm run build && npm run lint && npm run jest && pytest"
    },
    "dependencies": {
        "@babel/polyfill": "^7.4.4",
        "connected-react-router": "^6.5.0",
        "history": "^4.9.0",
        "isomorphic-fetch": "^2.2.1",
        "lodash": "^4.17.11",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-redux": "^7.1.0",
        "react-router": "^5.0.1",
        "react-router-dom": "^5.0.1",
        "redux": "^4.0.1",
        "redux-form": "^8.2.4",
        "redux-freeze": "^0.1.7",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.3.0",
        "webpack": "4.35.2",
        "webpack-cli": "^3.3.5",
        "webpack-merge": "^4.2.1"
    },
    "devDependencies": {
        "@babel/core": "^7.5.0",
        "@babel/preset-env": "^7.5.0",
        "@babel/preset-react": "^7.0.0",
        "@material-ui/core": "^4.2.0",
        "@material-ui/icons": "^4.2.1",
        "antd": "^3.20.1",
        "babel-core": "^7.0.0-bridge.0",
        "babel-eslint": "^10.0.2",
        "babel-jest": "^24.8.0",
        "babel-loader": "^8.0.6",
        "babel-preset-react-hmre": "^1.1.1",
        "clean-webpack-plugin": "^3.0.0",
        "compression-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^5.0.3",
        "css-loader": "^3.0.0",
        "eslint": "^6.0.1",
        "eslint-plugin-react": "^7.14.2",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^4.0.0",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^24.8.0",
        "json-loader": "^0.5.7",
        "jsx-loader": "^0.13.2",
        "less": "^3.9.0",
        "less-loader": "^5.0.0",
        "mini-css-extract-plugin": "^0.7.0",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "prop-types": "^15.7.2",
        "react-test-renderer": "^16.8.6",
        "style-loader": "^0.23.1",
        "typeface-roboto": "0.0.54",
        "uglifyjs-3-webpack-plugin": "^1.2.4",
        "webpack-dev-server": "^3.7.2"
    },
    "jest": {
        "testEnvironment": "node"
    }
}
